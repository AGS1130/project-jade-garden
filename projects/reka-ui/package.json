{
  "name": "@project-jade-garden/reka-ui",
  "version": "0.0.1",
  "description": "Reka UI anatomy slots for headless design",
  "keywords": [
    "reka-ui",
    "tailwindcss",
    "pandacss",
    "typescript",
    "tailwind",
    "panda",
    "anatomy",
    "parts",
    "slots",
    "types",
    "headless",
    "headless design",
    "vue",
    "vuejs",
    "ui"
  ],
  "author": "Gregory Salinas <ags1130@yahoo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGS1130/project-jade-garden.git",
    "directory": "projects/reka-ui"
  },
  "bugs": {
    "url": "https://github.com/AGS1130/project-jade-garden/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "bun typecheck && vite build",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "vite build --watch",
    "format": "biome format --write .",
    "lint": "biome lint",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "release-it": "release-it --config ../../release-it.json",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "clean-package": "2.2.0",
    "es-toolkit": "1.39.9",
    "globby": "14.1.0",
    "jade-garden": "1.1.1",
    "reka-ui": "2.3.0",
    "typescript": "5.9.2",
    "vite": "7.1.2",
    "vite-plugin-dts": "4.5.4"
  }
}
