          "types": "./dist/cva.d.ts",
          "default": "./dist/cva.js"
        },
        "require": {
          "types": "./dist/cva.d.cts",
          "default": "./dist/cva.cjs"
        }
      },
      "./sva": {
        "import": {
          "types": "./dist/sva.d.ts",
          "default": "./dist/sva.js"
