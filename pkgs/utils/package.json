{
  "name": "@spark-css/utils",
  "version": "0.5.1",
  "description": "Utilities to construct slots for Spark CSS",
  "keywords": ["ark-ui", "tailwindcss", "spark-css", "utilities"],
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./anatomy": {
      "import": "./dist/anatomy.js",
      "require": "./dist/anatomy.cjs",
      "types": "./dist/anatomy.d.ts"
    },
    "./create-docs": {
      "import": "./dist/create-docs.js",
      "require": "./dist/create-docs.cjs",
      "types": "./dist/create-docs.d.ts"
    },
    "./create-slots": {
      "import": "./dist/create-slots.js",
      "require": "./dist/create-slots.cjs",
      "types": "./dist/create-slots.d.ts"
    }
  },
  "typesVersions": {
    ">4.0": {
      ".": ["./dist/index.d.ts"],
      "anatomy": ["./dist/anatomy.d.ts"],
      "create-docs": ["./dist/create-docs.d.ts"],
      "create-slots": ["./dist/create-slots.d.ts"]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun typecheck && vite build",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "biome format --write ./src ./tests",
    "lint": "biome lint ./src ./tests",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zag-js/anatomy": "0.82.0",
    "es-toolkit": "1.31.0"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "vite": "6.0.11",
    "vite-plugin-dts": "4.5.0"
  },
  "clean-package": "../../clean-package.config.json"
}
