{
  "name": "@spark-css/utils",
  "version": "0.5.1",
  "description": "Utilities to construct slots for Spark CSS",
  "keywords": ["ark-ui", "tailwindcss", "spark-css", "utilities"],
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./anatomy": {
      "source": "./src/anatomy.ts",
      "import": {
        "types": "./dist/anatomy.d.ts",
        "default": "./dist/anatomy.js"
      },
      "require": {
        "types": "./dist/anatomy.d.cts",
        "default": "./dist/anatomy.cjs"
      }
    },
    "./create-docs": {
      "source": "./src/create-docs.ts",
      "import": {
        "types": "./dist/create-docs.d.ts",
        "default": "./dist/create-docs.js"
      },
      "require": {
        "types": "./dist/create-docs.d.cts",
        "default": "./dist/create-docs.cjs"
      }
    },
    "./create-slots": {
      "source": "./src/create-slots.ts",
      "import": {
        "types": "./dist/create-slots.d.ts",
        "default": "./dist/create-slots.js"
      },
      "require": {
        "types": "./dist/create-slots.d.cts",
        "default": "./dist/create-slots.cjs"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun typecheck && vite build",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "biome format --write ./src ./tests",
    "lint": "biome lint ./src ./tests",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zag-js/anatomy": "0.82.1",
    "es-toolkit": "1.32.0"
  },
  "devDependencies": {
    "clean-package": "2.2.0",
    "globby": "14.0.2",
    "vite": "6.0.11",
    "vite-plugin-dts": "4.5.0"
  },
  "clean-package": "../../clean-package.config.json"
}
