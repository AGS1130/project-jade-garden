{
  "name": "unplugin-jade-garden",
  "version": "0.0.0",
  "description": "Build tool plugin for creating CSS with Jade Garden",
  "keywords": [
    "tailwindcss",
    "tailwind",
    "class-variance-authority",
    "slots-variance-authority",
    "classes",
    "classname",
    "classnames",
    "css",
    "cva",
    "sva",
    "styles",
    "variants"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGS1130/project-jade-garden.git",
    "directory": "tools/unplugin-jade-garden"
  },
  "bugs": {
    "url": "https://github.com/AGS1130/project-jade-garden/issues"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./rollup": {
      "source": "./src/rollup.ts",
      "import": {
        "types": "./dist/rollup.d.ts",
        "default": "./dist/rollup.js"
      },
      "require": {
        "types": "./dist/rollup.d.cts",
        "default": "./dist/rollup.cjs"
      }
    },
    "./rspack": {
      "source": "./src/rspack.ts",
      "import": {
        "types": "./dist/rspack.d.ts",
        "default": "./dist/rspack.js"
      },
      "require": {
        "types": "./dist/rspack.d.cts",
        "default": "./dist/rspack.cjs"
      }
    },
    "./vite": {
      "source": "./src/vite.ts",
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    },
    "./webpack": {
      "source": "./src/webpack.ts",
      "import": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.js"
      },
      "require": {
        "types": "./dist/webpack.d.cts",
        "default": "./dist/webpack.cjs"
      }
    }
  },
  "scripts": {
    "build": "bun typecheck && tsup",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "tsup --watch",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "clean-package": {
    "remove": ["exports.\\..source", "exports.\\./*.source"]
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "es-toolkit": "1.36.0",
    "unplugin": "2.3.2",
    "unplugin-utils": "0.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rspack/core": "1.3.5",
    "@tailwindcss/postcss": "4.1.4",
    "clean-package": "2.2.0",
    "css-loader": "7.1.2",
    "jade-garden": "workspace:*",
    "postcss-loader": "8.1.1",
    "postcss": "8.5.3",
    "rollup-plugin-postcss": "4.0.2",
    "rollup": "4.40.0",
    "tailwindcss": "4.1.4",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "webpack": "5.99.6"
  }
}
