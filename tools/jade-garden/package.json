{
  "name": "jade-garden",
  "version": "1.1.2",
  "description": "Class utilities to compose class names and variants",
  "keywords": [
    "tailwindcss",
    "tailwind",
    "class-variance-authority",
    "slots-variance-authority",
    "classes",
    "classname",
    "classnames",
    "css",
    "cva",
    "sva",
    "styles",
    "variants"
  ],
  "author": "Gregory Salinas <ags1130@yahoo.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AGS1130/project-jade-garden.git",
    "directory": "tools/jade-garden"
  },
  "bugs": {
    "url": "https://github.com/AGS1130/project-jade-garden/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "bun typecheck && vite build",
    "clean": "rm -rf .turbo dist node_modules",
    "dev": "vite build --watch",
    "format": "biome format --write .",
    "lint": "biome lint",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "clsx": "2.1.1",
    "es-toolkit": "1.39.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "clean-package": "2.2.0",
    "globby": "14.1.0",
    "tailwind-merge": "3.3.1",
    "typescript": "5.9.2",
    "vite": "7.1.3",
    "vite-plugin-dts": "4.5.4"
  }
}
